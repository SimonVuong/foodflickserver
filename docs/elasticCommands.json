{
  "script": {
    "source": """
        ctx.op = "noop";
        def menu = ctx._source.menu;
        for (int i = 0; i < menu.length; i++) {
          if (menu[i].name.equals(params.categoryName)) {
            List items = menu[i].items;
            for (int j = 0; j < items.length; j++) {
              if (items[j].name.equals(params.itemName)) {
                items[j].likes++;
                ctx.op = "index";
                break;
              }
            }
          }
        }
    """,
